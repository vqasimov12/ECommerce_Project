<Page x:Class="ECommerce_Project.Views.CommonViews.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ECommerce_Project.Views.CommonViews"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page1">

    <Grid>
        <syncfusion:SfRating ItemsCount="5" Precision="Exact" Width="150"
                             Value="{Binding Rating, Mode=TwoWay}" />
    </Grid>
</Page>
